{
    "name": "rennwelten-ai-steward",
    "description": "A smart Tool that supports Sim Racing Stewards during the stewarding process",
    "license": "MIT",
    "version": "1.0.14",
    "main": "./dist/src/main.js",
    "icon": "./dist/favicon.ico",
    "expressPort": 3000,
    "basePath": "./dist",
    "authors": [
        "Asappz UG (Haftungsbeschr√§nkt) Julian Sunten <js@asappz.com>"
    ],
    "repository": "https://github.com/juliansunten/rennwelten-ai-steward-new.git",
    "private": true,
    "scripts": {
        "build": "build:client && tsc && yarn copy:client-to-server",
        "build:client": "cd src/client && npm run build:prod",
        "start:client": "cd src/client && npm run start",
        "buildandstart:elektron": "npm run build:server && electron main",
        "start:electron": "electron ./dist/src/main --trace-warnings",
        "copy:client-to-server": "cp -r -f -a src/client/dist/rennwelten-ai-steward/. ./dist/public",
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish"
    },
    "dependencies": {
        "body-parser": "^1.20.2",
        "cookie-parser": "^1.4.6",
        "debug": "^4.3.4",
        "ejs": "^3.1.9",
        "electron-squirrel-startup": "^1.0.0",
        "express": "^4.18.2",
        "http-errors": "^2.0.0",
        "morgan": "^1.10.0",
        "node-fetch": "^2.7.0",
        "serve-favicon": "^2.5.0",
        "update-electron-app": "^2.0.1"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.4.2",
        "@electron-forge/maker-deb": "^6.4.2",
        "@electron-forge/maker-rpm": "^6.4.2",
        "@electron-forge/maker-squirrel": "^6.4.2",
        "@electron-forge/maker-zip": "^6.4.2",
        "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
        "@types/body-parser": "^1.19.3",
        "@types/cookie-parser": "^1.4.3",
        "@types/express": "^4.17.17",
        "@types/http-errors": "^2.0.1",
        "@types/morgan": "^1.9.5",
        "@types/node": "^20.5.0",
        "@types/node-fetch": "^2.6.0",
        "electron": "^26.1.0",
        "typescript": "^5.2.2"
    }
}
